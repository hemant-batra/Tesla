<html>
<head>
<script type="text/javascript">
function loadPage(pageId)
{
	var password = document.getElementById('idPassword').value;
	if(pageId=='documentation')
		document.location.href = '/Documentation';
	else if(pageId=='resources')
		document.location.href = '/Resources?password='+password;
	else if(pageId=='shutdown')
		document.location.href = '/Shutdown?password='+password;
}
</script>
</head>
<body>
<br><br><br>
<div align="center" style="font-family:calibri;">
<span style="font-size:18pt;">Tesla</span><br><br>
<span style="font-size:14pt;"><input type="password" placeholder="Password" id="idPassword"/></span><br><br>
<span style="font-size:14pt;color:blue;cursor:hand;" onclick="loadPage('documentation');">Server Documentation</span><br>
<span style="font-size:14pt;color:blue;cursor:hand;" onclick="loadPage('resources');">Deployed Resources<font color="red">*</font></span><br>
<span style="font-size:14pt;color:blue;cursor:hand;" onclick="loadPage('shutdown');">Shutdown Server<font color="red">*</font></span><br><br>
<span style="font-size:14pt;"><font color="red">*</font> Require password</span>
</div>
</body>
</html>